%p== #{session.created_at.try(:strftime, '%b %d, %Y')} on #{session.machine.display_name}
= image_tag session.patient_image_url(:standard).to_s
- if session.labels.empty?
  %h3 Measurements
  = render :partial => 'measurements', :locals => { :measurements => session.measurements }
- else
  - width = 9 / session.labels.count
  - session.labels.each do |label|
    .span#{width}
      %h3== Measurements (#{label})
      = render :partial => 'measurements', :locals => { :measurements => session.labeled_measurements(label) }
%h2 Treatments
- session.treatments.order('created_at desc').each do |treatment|
  == #{treatment.duration_minutes} minutes, #{treatment.protocol.name}: #{treatment.process_timer.process_state}
