.facilitypage
  = form_for @protocol do |f|
    = render :partial => 'shared/error_messages', :locals => { :errors => @protocol.errors, :name => 'Protocol' }
    = render :partial => 'fields', :locals => { :f => f, :testimonial => @protocol }
    .actions
      = f.submit 'Update Protocol', :class => 'btn btn-large btn-success'
